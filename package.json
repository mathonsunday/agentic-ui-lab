{
  "name": "agentic-ui-lab",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.12.0"
  },
  "scripts": {
    "dev": "tsc -b --noEmit && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc -b --noEmit",
    "type:coverage": "type-coverage --detail --strict",
    "type:report": "typescript-coverage-report",
    "dead-code": "knip",
    "dead-code:fix": "knip --fix",
    "check-deps": "depcruise src api --config",
    "verify": "npm run type-check && npm run lint && npm run dead-code && npm run check-deps && npm test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@mathonsunday/ascii-art-toolkit": "^0.2.0",
    "jotai": "^2.15.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vercel/node": "^3.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "dependency-cruiser": "^17.3.6",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "happy-dom": "^20.3.1",
    "husky": "^9.1.7",
    "knip": "^5.82.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "type-coverage": "^2.29.7",
    "typescript": "~5.9.3",
    "typescript-coverage-report": "^1.1.1",
    "typescript-eslint": "^8.46.4",
    "vite": "^5.4.0",
    "vitest": "^4.0.17"
  }
}
